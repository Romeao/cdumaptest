      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAjeXslms5w82HAfl5unpcWBvaslnFi0gc"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "CDU Map app",
    "githubUrl": "https://github.com/Romeao/cdumaptest.git"
  }
}
